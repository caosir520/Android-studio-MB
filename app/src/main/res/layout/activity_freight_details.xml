<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="net.ed58.dlm.clients.order.FreightDetailsActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/icon_shutdown_bulletbox"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/x30"
        android:layout_marginLeft="@dimen/x30"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="@dimen/x30" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="57dp"
        android:text="运费明细"
        android:textColor="@color/title_text_color"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="@dimen/x30" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:includeFontPadding="true"
        android:paddingBottom="0dp"
        android:text="10"
        android:textColor="@color/title_text_color"
        android:textSize="@dimen/x48"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView12"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        android:layout_marginLeft="167dp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/x30"
        android:text="元"
        android:textColor="@color/title_text_color"
        android:textSize="@dimen/x30"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="34dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/x30"
        android:layout_marginTop="@dimen/y90"
        android:text="需要支付金额"
        android:textColor="@color/title_text_color"
        android:textSize="@dimen/x30"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        android:layout_marginStart="@dimen/x30" />

    <View
        android:id="@+id/view5"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="@dimen/x30"
        android:layout_marginEnd="@dimen/x30"
        android:layout_marginTop="@dimen/y40"
        android:background="@color/cut_off_color"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="订单距离"
        android:textColor="@color/text_color"
        android:textSize="@dimen/x28"
        app:layout_constraintTop_toBottomOf="@+id/view5"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="@dimen/x30" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="231dp"
        android:layout_marginTop="20dp"
        android:text="2.8"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        app:layout_constraintRight_toLeftOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/view5"
        android:layout_marginStart="231dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="公里"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/view5"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="@dimen/x30" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/cut_off_color"
        android:layout_marginTop="@dimen/y40"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="@dimen/x30"
        android:layout_marginEnd="@dimen/x30"
        app:layout_constraintHorizontal_bias="0.0"
        android:id="@+id/view6" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y40"
        android:text="基础运费"
        app:layout_constraintTop_toBottomOf="@+id/view6"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="@dimen/x30" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="10"
        app:layout_constraintRight_toLeftOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/view6" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="元"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/view6"
        android:layout_marginEnd="@dimen/x30" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/cut_off_color"
        android:layout_marginTop="@dimen/y40"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="@dimen/x30"
        android:layout_marginEnd="@dimen/x30"
        app:layout_constraintHorizontal_bias="0.0"
        android:id="@+id/view7"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="加价"
        android:layout_marginTop="@dimen/y40"
        app:layout_constraintTop_toBottomOf="@+id/view7"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="@dimen/x30" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="0"
        app:layout_constraintRight_toLeftOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="元"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/view7"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="@dimen/x30" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/cut_off_color"
        android:layout_marginTop="@dimen/y40"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="@dimen/x30"
        android:layout_marginEnd="@dimen/x30"
        app:layout_constraintHorizontal_bias="0.0"
        android:id="@+id/view8"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/in_input_home"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/y40"
        app:layout_constraintTop_toBottomOf="@+id/view8"
        android:layout_marginEnd="@dimen/x30" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="运费计价规则"
        android:layout_marginTop="17dp"
        android:textColor="@color/font_blue"
        android:textSize="@dimen/x24"
        app:layout_constraintTop_toBottomOf="@+id/view8"
        app:layout_constraintRight_toLeftOf="@+id/imageView7" />


</android.support.constraint.ConstraintLayout>
